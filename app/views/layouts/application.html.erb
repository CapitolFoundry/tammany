<!DOCTYPE html>
<html>
<head>
  <title>Tammany</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  
  <% if caseworker_signed_in? %>
    <%= link_to('Logout', destroy_caseworker_session_path, :method => :delete) %>        
  <% else %>
    <%= link_to('Login', new_caseworker_session_path)  %>  
  <% end %>

<%= yield %>

</body>
</html>
